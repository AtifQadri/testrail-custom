name: Lock specific test case fields from editing
description: Disables test case field editing based on a user's role_id
author: Gurock Software
version: 1.0
includes: ^cases/edit
excludes: 

js:
$(document).ready(
	function() {
		user_role = data = uiscripts.context.user.role_id;
		if (user_role == 2)
		{
			//May need to add a check to see if the field exists
			document.getElementById("template_id").disabled = true;
		}
	}
);
